<head>
  <link href="/styles/styles.css" rel="stylesheet">
</head>
<body>
  <div class="container">
    <form class="login">
      <div class="form-group">
        <label>Username:</label>
        <input type="username" class="form-control" id="username-input" placeholder="Username">
      </div>
      <div class="form-group">
        <label>Password:</label>
        <input type="password" class="form-control" id="password-input" placeholder="Password">
      </div>
        <button type="submit" class="btn btn-default" id="login-submit">Login</button>
        <button type="submit" class="btn btn-default" id="signup-submit">Signup</button>
    </form>
  </div>
</body>
<script src="./js/login.js" type="text/javascript"></script>
